{
  "name": "@coinbase/cds-common",
  "version": "8.26.0",
  "description": "Coinbase Design System - Common",
  "repository": {
    "type": "git",
    "url": "git@github.com:coinbase/cds.git",
    "directory": "packages/common"
  },
  "react-native": "./src/index.ts",
  "type": "module",
  "main": "./esm/index.js",
  "types": "./dts/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dts/index.d.ts",
      "default": "./esm/index.js"
    },
    "./types": {
      "types": "./dts/types/index.d.ts",
      "default": "./esm/types/index.js"
    },
    "./*": {
      "types": "./dts/*.d.ts",
      "default": "./esm/*.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dts",
    "esm",
    "mcp-docs",
    "CHANGELOG"
  ],
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "dependencies": {
    "@coinbase/cds-icons": "workspace:^",
    "@coinbase/cds-illustrations": "workspace:^",
    "@coinbase/cds-mcp-server": "workspace:^",
    "@coinbase/cds-utils": "workspace:^",
    "@modelcontextprotocol/sdk": "^1.13.1",
    "d3-array": "^3.2.4",
    "d3-color": "^3.1.0",
    "d3-interpolate": "^3.0.1",
    "d3-scale": "^4.0.2",
    "d3-shape": "^3.2.0",
    "lodash": "^4.17.21",
    "react-is": "^17.0.2",
    "type-fest": "^2.19.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@types/react": "^18.3.12",
    "jest-date-mock": "^1.0.8"
  }
}
