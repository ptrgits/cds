{
  "name": "@coinbase/cds-illustrations",
  "version": "4.26.1",
  "description": "CDS illustrations",
  "repository": {
    "type": "git",
    "url": "git@github.com:coinbase/cds.git",
    "directory": "packages/illustrations"
  },
  "type": "module",
  "main": "./esm/index.js",
  "types": "./dts/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dts/index.d.ts",
      "default": "./esm/index.js"
    },
    "./*": {
      "types": "./dts/*.d.ts",
      "default": "./esm/*.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dts",
    "esm",
    "CHANGELOG"
  ],
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "chalk": "^4.1.2",
    "prettier": "^3.6.2"
  }
}
